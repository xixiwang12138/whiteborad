<!--square.wxml-->
<view class="container">
  <view class="container-top">
    <avatar-display avatar="../../assets/square/room.jpg" name="Exermon" />
  </view>
  <gold-level-display gold="50000" level="99" expRate="0.2333"/>
  <view class="search">
    <input value="输入id搜索" />
    <image class="img" src="../../assets/square/search.png" mode="widthFix"></image>
  </view>
  <scroll-view class="room-list" scroll-y="true">
    <view class="room-list-content">
      <view class="room" wx:for="{{roomList}}">
        <image src="{{item.thumbnail}}" mode="widthFix" class="room-thumbnail"></image>
        <view class="room-desc">
          <text class="room-nickName">{{item.nickName}}'s</text>
          <view class="room-level-name">
            <text class="room-level">{{item.level}}</text>
            <text class="room-name">{{item.name}}</text>
          </view>
        </view>

      </view>
    </view>
  </scroll-view>

</view>
<real-tab-bar curPageIndex="1"></real-tab-bar>
